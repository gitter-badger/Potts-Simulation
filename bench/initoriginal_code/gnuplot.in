reset

set terminal pdfcairo dashed enhanced color dashed size 14cm,13cm font 'Verdana,14'
set key left top
set key spacing 1.5
set key samplen 5

set title "Code time partitioning (Cm=300,S=3,p=10)"

set key invert reverse Left outside
set key autotitle columnheader

set yrange [0:100]
set ylabel "% of total"
set xlabel "N"

set style data histogram
set style histogram rowstacked
set style fill solid border -1
set boxwidth 0.75

set output "timepart.pdf"

plot 'values.dat' using (100*$1/($1+$2+$3)):xtic(5) title "patterns"\
    ,'' using (100*$2/($1+$2+$3)) t "initialization"\
    ,'' using (100*$3/($1+$2+$3)) t "dynamics(100 tsteps)"
